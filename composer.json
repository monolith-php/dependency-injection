{
  "name": "monolith/dependency-injection",
  "description": "Dependency injection container for the Monolith framework.",
  "keywords": [
    "monolith",
    "dependency injection",
    "container"
  ],
  "homepage": "https://www.getmonolith.com",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "monolith/collections": "^2",
    "psr/container": "^1"
  },
  "require-dev": {
    "phpspec/phpspec": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\DependencyInjection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\DependencyInjection\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
