{
  "name": "monolith/dependency-injection",
  "description": "Dependency injection container for the Monolith framework.",
  "keywords": [
    "monolith",
    "dependency injection",
    "container"
  ],
  "require": {
    "php": ">=8.1",
    "monolith/collections": "^10",
    "psr/container": "^1"
  },
  "require-dev": {
    "phpspec/phpspec": "^7"
  },
  "autoload": {
    "psr-4": {
      "Monolith\\DependencyInjection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Monolith\\DependencyInjection\\": "spec/"
    },
    "files": [
      "spec/functions.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "minimum-stability": "dev"
}
